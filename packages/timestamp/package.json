{
  "name": "@angular-builders/timestamp",
  "version": "12.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/just-jeb/angular-builders",
    "directory": "packages/timestamp"
  },
  "author": "JeB Barabanov",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist",
    "builders.json"
  ],
  "engines": {
    "node": ">=12.14.1"
  },
  "builders": "builders.json",
  "scripts": {
    "prebuild": "yarn clean && yarn generate",
    "build": "tsc",
    "postbuild": "yarn copy",
    "clean": "rimraf dist src/schema.ts",
    "copy": "cpy src/schema.json dist",
    "generate": "quicktype -s schema src/schema.json -o src/schema.ts"
  },
  "devDependencies": {
    "@types/dateformat": "^3.0.1",
    "rimraf": "^3.0.0",
    "cpy-cli": "^3.0.0",
    "quicktype": "^15.0.174",
    "typescript": "4.0"
  },
  "dependencies": {
    "@angular-devkit/architect": ">=0.1200.0 < 0.1300.0",
    "@angular-devkit/core": "^12.0.0",
    "dateformat": "^4.3.1"
  }
}
